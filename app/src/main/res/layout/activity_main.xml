<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="16dp">

    <TextView
        android:id="@+id/aes_128_caption"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/aes_128_cipher_caption"
        android:gravity="center"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <TextView
        android:id="@+id/aes_256_caption"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:text="@string/aes_256_cipher_caption"
        android:gravity="center"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toRightOf="@id/aes_128_caption"/>

    <TextView
        android:id="@+id/blowfish_128_caption"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:text="@string/blowfish_128_cipher_caption"
        android:gravity="center"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toRightOf="@id/aes_256_caption"/>

    <TextView
        android:id="@+id/blowfish_256_caption"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:text="@string/blowfish_128_cipher_caption"
        android:gravity="center"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toRightOf="@id/blowfish_128_caption"/>

    <TextView
        android:id="@+id/chacha20_caption"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:text="@string/chacha20_cipher_caption"
        android:gravity="center"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toRightOf="@id/blowfish_256_caption"/>

    <!--  Ciphering small data  -->

    <TextView
        android:id="@+id/total_time_small_data_cipher_caption"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="@string/total_time_small_data_cipher_caption"
        app:layout_constraintTop_toBottomOf="@id/blowfish_128_caption"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        />

    <TextView
        android:id="@+id/aes_128_small_time_cipher_result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="0"
        app:layout_constraintTop_toBottomOf="@id/total_time_small_data_cipher_caption"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <TextView
        android:id="@+id/aes_256_small_time_cipher_result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="0"
        app:layout_constraintTop_toBottomOf="@id/total_time_small_data_cipher_caption"
        app:layout_constraintLeft_toLeftOf="@id/aes_256_caption"/>

    <TextView
        android:id="@+id/blowfish_128_small_time_cipher_result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="0"
        app:layout_constraintTop_toBottomOf="@id/total_time_small_data_cipher_caption"
        app:layout_constraintLeft_toLeftOf="@id/blowfish_128_caption"/>

    <TextView
        android:id="@+id/blowfish_256_small_time_cipher_result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="0"
        app:layout_constraintTop_toBottomOf="@id/total_time_small_data_cipher_caption"
        app:layout_constraintLeft_toLeftOf="@id/blowfish_256_caption"/>

    <TextView
        android:id="@+id/chacha20_small_time_cipher_result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="0"
        app:layout_constraintTop_toBottomOf="@id/total_time_small_data_cipher_caption"
        app:layout_constraintLeft_toLeftOf="@id/chacha20_caption"/>

    <!--  Deciphering small data  -->

    <TextView
        android:id="@+id/total_time_small_data_decipher_caption"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="@string/total_time_small_data_decipher_caption"
        app:layout_constraintTop_toBottomOf="@id/blowfish_128_small_time_cipher_result"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        />

    <TextView
        android:id="@+id/aes_128_small_time_decipher_result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="0"
        app:layout_constraintTop_toBottomOf="@id/total_time_small_data_decipher_caption"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <TextView
        android:id="@+id/aes_256_small_time_decipher_result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="0"
        app:layout_constraintTop_toBottomOf="@id/total_time_small_data_decipher_caption"
        app:layout_constraintLeft_toLeftOf="@id/aes_256_caption"/>

    <TextView
        android:id="@+id/blowfish_128_small_time_decipher_result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="0"
        app:layout_constraintTop_toBottomOf="@id/total_time_small_data_decipher_caption"
        app:layout_constraintLeft_toLeftOf="@id/blowfish_128_caption"/>

    <TextView
        android:id="@+id/blowfish_256_small_time_decipher_result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="0"
        app:layout_constraintTop_toBottomOf="@id/total_time_small_data_decipher_caption"
        app:layout_constraintLeft_toLeftOf="@id/blowfish_256_caption"/>

    <TextView
        android:id="@+id/chacha20_small_time_decipher_result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="0"
        app:layout_constraintTop_toBottomOf="@id/total_time_small_data_decipher_caption"
        app:layout_constraintLeft_toLeftOf="@id/chacha20_caption"/>

    <!--  Ciphering large data  -->

    <TextView
        android:id="@+id/total_time_large_data_cipher_caption"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="@string/total_time_large_data_cipher_caption"
        app:layout_constraintTop_toBottomOf="@id/blowfish_128_small_time_decipher_result"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        />

    <TextView
        android:id="@+id/aes_128_large_time_cipher_result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="0"
        app:layout_constraintTop_toBottomOf="@id/total_time_large_data_cipher_caption"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <TextView
        android:id="@+id/aes_256_large_time_cipher_result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="0"
        app:layout_constraintTop_toBottomOf="@id/total_time_large_data_cipher_caption"
        app:layout_constraintLeft_toLeftOf="@id/aes_256_caption"/>

    <TextView
        android:id="@+id/blowfish_128_large_time_cipher_result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="0"
        app:layout_constraintTop_toBottomOf="@id/total_time_large_data_cipher_caption"
        app:layout_constraintLeft_toLeftOf="@id/blowfish_128_caption"/>

    <TextView
        android:id="@+id/blowfish_256_large_time_cipher_result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="0"
        app:layout_constraintTop_toBottomOf="@id/total_time_large_data_cipher_caption"
        app:layout_constraintLeft_toLeftOf="@id/blowfish_256_caption"/>

    <TextView
        android:id="@+id/chacha20_large_time_cipher_result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="0"
        app:layout_constraintTop_toBottomOf="@id/total_time_large_data_cipher_caption"
        app:layout_constraintLeft_toLeftOf="@id/chacha20_caption"/>

    <!--  Deciphering large data  -->

    <TextView
        android:id="@+id/total_time_large_data_decipher_caption"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="@string/total_time_large_data_decipher_caption"
        app:layout_constraintTop_toBottomOf="@id/blowfish_128_large_time_cipher_result"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        />

    <TextView
        android:id="@+id/aes_128_large_time_decipher_result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="0"
        app:layout_constraintTop_toBottomOf="@id/total_time_large_data_decipher_caption"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <TextView
        android:id="@+id/aes_256_large_time_decipher_result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="0"
        app:layout_constraintTop_toBottomOf="@id/total_time_large_data_decipher_caption"
        app:layout_constraintLeft_toLeftOf="@id/aes_256_caption"/>

    <TextView
        android:id="@+id/blowfish_128_large_time_decipher_result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="0"
        app:layout_constraintTop_toBottomOf="@id/total_time_large_data_decipher_caption"
        app:layout_constraintLeft_toLeftOf="@id/blowfish_128_caption"/>

    <TextView
        android:id="@+id/blowfish_256_large_time_decipher_result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="0"
        app:layout_constraintTop_toBottomOf="@id/total_time_large_data_decipher_caption"
        app:layout_constraintLeft_toLeftOf="@id/blowfish_256_caption"/>

    <TextView
        android:id="@+id/chacha20_large_time_decipher_result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="0"
        app:layout_constraintTop_toBottomOf="@id/total_time_large_data_decipher_caption"
        app:layout_constraintLeft_toLeftOf="@id/chacha20_caption"/>

    <Button
        android:id="@+id/start_button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@id/blowfish_128_large_time_decipher_result"
        android:text="@string/start_button_caption"/>

</androidx.constraintlayout.widget.ConstraintLayout>